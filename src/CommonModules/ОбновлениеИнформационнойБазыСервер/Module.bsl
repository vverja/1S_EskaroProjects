	Возврат Запрос.Выполнить().Выгрузить();
	
КонецФункции // ПолучитьТаблицуСправочниковДокументов()

Функция ПолучитьНалоговоеНазначениеНДС(НалоговоеНазначениеНДССтарое, СоответствиеНалоговыхНазначенийНДС, ВидДеятельностиНДССтарый = Неопределено) 
	
	
	Результат = Неопределено;
	
	Если СоответствиеНалоговыхНазначенийНДС <> Неопределено Тогда
		Результат = СоответствиеНалоговыхНазначенийНДС.Получить(НалоговоеНазначениеНДССтарое);
	КонецЕсли;	
		
	
	Если Результат = Неопределено Тогда
		Если НалоговоеНазначениеНДССтарое <> Неопределено Тогда
			ВидДеятельностиНДС = НалоговоеНазначениеНДССтарое.ВидДеятельностиНДС;
		Иначе
			ВидДеятельностиНДС = ВидДеятельностиНДССтарый;	
		КонецЕсли;
		
		Если Не ЗначениеЗаполнено(ВидДеятельностиНДС) Тогда
			Возврат Справочники.НалоговыеНазначенияАктивовИЗатрат.ПустаяСсылка();
		КонецЕсли; 
		
		Если ВидДеятельностиНДС = Перечисления.ВидыДеятельностиНДС.Необлагаемая Тогда
			Результат = Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_НеоблагаемаяХозДеятельность;
		Иначе
			Результат = Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_Облагаемая;
		КонецЕсли; 
		Если СоответствиеНалоговыхНазначенийНДС <> Неопределено Тогда
			СоответствиеНалоговыхНазначенийНДС.Вставить(НалоговоеНазначениеНДССтарое, Результат);
		КонецЕсли;	
	КонецЕсли;
	
	Возврат Результат;

КонецФункции // ПолучитьНалоговоеНазначениеНДС()

Функция ПолучитьНалоговоеНазначениеДоходовИЗатрат(НалоговоеНазначениеДоходовИЗатратСтарое, СоответствиеНалоговыхНазначенийДоходовИЗатрат, ВидНалоговойДеятельностиСтарый = Неопределено)
	
	Результат = Неопределено;
	
	Если СоответствиеНалоговыхНазначенийДоходовИЗатрат <> Неопределено Тогда
		Результат = СоответствиеНалоговыхНазначенийДоходовИЗатрат.Получить(НалоговоеНазначениеДоходовИЗатратСтарое);
	КонецЕсли;	
	
	Если Результат = Неопределено Тогда
		// ищем налоговое назначение с таким же видом налоговой деятельности, группой доходов и затрат
		Запрос = Новый Запрос;
		Запрос.Текст = "ВЫБРАТЬ ПЕРВЫЕ 1
		|	НалоговыеНазначенияАктивовИЗатрат.Ссылка КАК НалоговоеНазначение
		|ИЗ
		|	Справочник.НалоговыеНазначенияАктивовИЗатрат КАК НалоговыеНазначенияАктивовИЗатрат
		|ГДЕ
		|	НалоговыеНазначенияАктивовИЗатрат.ВидНалоговойДеятельности = &ВидНалоговойДеятельности
		|	И НалоговыеНазначенияАктивовИЗатрат.ГруппаНалоговогоНазначения = ЗНАЧЕНИЕ(Перечисление.ГруппыНалоговыхНазначений.НалоговыеНазначенияДоходовИЗатрат)
		|";
		
		Если НалоговоеНазначениеДоходовИЗатратСтарое <> Неопределено Тогда
			ВидНалоговойДеятельности = НалоговоеНазначениеДоходовИЗатратСтарое.ВидНалоговойДеятельности;
		Иначе
			ВидНалоговойДеятельности = ВидНалоговойДеятельностиСтарый;
		КонецЕсли;	
		
		Если Не ЗначениеЗаполнено(ВидНалоговойДеятельности) Тогда
			Возврат Справочники.НалоговыеНазначенияАктивовИЗатрат.ПустаяСсылка();
		КонецЕсли; 
		
		Запрос.УстановитьПараметр("ВидНалоговойДеятельности", ВидНалоговойДеятельности); 
		
		РезультатЗапроса = Запрос.Выполнить();
		Выборка = РезультатЗапроса.Выбрать();
		
		Если Выборка.Следующий() Тогда
			Результат = Выборка.НалоговоеНазначение;
		Иначе
			// создадим
			
			НовыйЭлемент = Справочники.НалоговыеНазначенияАктивовИЗатрат.СоздатьЭлемент();
			НовыйЭлемент.ВидНалоговойДеятельности = ВидНалоговойДеятельности;
			НовыйЭлемент.ГруппаНалоговогоНазначения = Перечисления.ГруппыНалоговыхНазначений.НалоговыеНазначенияДоходовИЗатрат;
			
			Наименование = "";
			Наименование = Наименование + ВидНалоговойДеятельности;
			
			НовыйЭлемент.Наименование = Наименование;
			
			НовыйЭлемент.ОбменДанными.Загрузка = Истина;
			НовыйЭлемент.Записать();
			Сообщить("Записано новое налоговое назначение " + Наименование);
			Результат = НовыйЭлемент.Ссылка;
			
		КонецЕсли;
		Если СоответствиеНалоговыхНазначенийДоходовИЗатрат <> Неопределено Тогда
			СоответствиеНалоговыхНазначенийДоходовИЗатрат.Вставить(НалоговоеНазначениеДоходовИЗатратСтарое, Результат);
		КонецЕсли;	
	КонецЕсли;
	
	Возврат Результат;

КонецФункции // ПолучитьНалоговоеНазначениеДоходовИЗатрат()

Процедура ПолучитьСоответствиеСтарыхИНовыхНалоговыхНазначений(ПоПрибыли = Неопределено, ПоНДС = Неопределено) 
	
	ПоНДС = Новый Соответствие;
	ПоНДС.Вставить(Справочники.НалоговыеНазначенияАктивовИЗатрат.НеХозДеятельность				   , Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_НеоблагаемаяНеХозДеятельность);
	ПоНДС.Вставить(Справочники.НалоговыеНазначенияАктивовИЗатрат.НеХозДеятельностьАморт            , Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_НеоблагаемаяНеХозДеятельность);
	ПоНДС.Вставить(Справочники.НалоговыеНазначенияАктивовИЗатрат.НеХозДеятельностьАмортНеОблагНДС  , Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_НеоблагаемаяНеХозДеятельность);
	ПоНДС.Вставить(Справочники.НалоговыеНазначенияАктивовИЗатрат.НеХозДеятельностьНеОблагНДС       , Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_НеоблагаемаяНеХозДеятельность);
	ПоНДС.Вставить(Справочники.НалоговыеНазначенияАктивовИЗатрат.ХозДеятельность                   , Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_Облагаемая);
	ПоНДС.Вставить(Справочники.НалоговыеНазначенияАктивовИЗатрат.ХозДеятельностьАморт              , Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_Облагаемая);
	ПоНДС.Вставить(Справочники.НалоговыеНазначенияАктивовИЗатрат.ХозДеятельностьАмортНеОблагНДС    , Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_НеоблагаемаяХозДеятельность);
	ПоНДС.Вставить(Справочники.НалоговыеНазначенияАктивовИЗатрат.ХозДеятельностьНеОблагНДС         , Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_НеоблагаемаяХозДеятельность);
	ПоНДС.Вставить(Справочники.НалоговыеНазначенияАктивовИЗатрат.ТоргДеятельность                  , Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_Облагаемая);
	ПоНДС.Вставить(Справочники.НалоговыеНазначенияАктивовИЗатрат.ТоргДеятельностьАморт             , Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_Облагаемая);
	ПоНДС.Вставить(Справочники.НалоговыеНазначенияАктивовИЗатрат.ТоргДеятельностьАмортНеОблагНДС   , Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_НеоблагаемаяХозДеятельность);
	ПоНДС.Вставить(Справочники.НалоговыеНазначенияАктивовИЗатрат.ТоргДеятельностьНеОблагНДС        , Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_НеоблагаемаяХозДеятельность);
	ПоНДС.Вставить(Справочники.НалоговыеНазначенияАктивовИЗатрат.БытУслуги                         , Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_Облагаемая);
	ПоНДС.Вставить(Справочники.НалоговыеНазначенияАктивовИЗатрат.БытУслугиАморт                    , Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_Облагаемая);
	ПоНДС.Вставить(Справочники.НалоговыеНазначенияАктивовИЗатрат.БытУслугиАмортНеОблагНДС          , Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_НеоблагаемаяХозДеятельность);
	ПоНДС.Вставить(Справочники.НалоговыеНазначенияАктивовИЗатрат.БытУслугиНеОблагНДС               , Справочники.НалоговыеНазначенияАктивовИЗатрат.НДС_НеоблагаемаяХозДеятельность);
