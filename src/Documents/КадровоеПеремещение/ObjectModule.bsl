	ЗапросПоСхемамМотивации.УстановитьПараметр("ВидСхемыМотивацииПустаяССылка",Справочники.ВариантыCхемМотивации.ПустаяСсылка());
	ЗапросПоСхемамМотивации.УстановитьПараметр("Неопределено",Неопределено);
	
	
	Если ВидТаблицы = "Начисления" Тогда
		Выборка = ЗапросПоСхемамМотивации.Выполнить().Выбрать();
		
		ПрежнийСотрудник = Справочники.СотрудникиОрганизаций.ПустаяСсылка();
		НомерСтрокиОснНачисления = 0;
		Начисления.Очистить();
		Пока Выборка.Следующий() Цикл
			
			Если ПрежнийСотрудник <> Выборка.Сотрудник Тогда
				ПрежнийСотрудник = Выборка.Сотрудник;
				НомерСтрокиОснНачисления = 0;
			КонецЕсли;

			СтрокаНачислений = Начисления.Добавить();
			СтрокаНачислений.Сотрудник	= Выборка.Сотрудник;
			СтрокаНачислений.Физлицо	= Выборка.Физлицо;
			СтрокаНачислений.ВидРасчета = Выборка.ВидРасчета;
			Если Выборка.СМВидРасчета = NULL Тогда
				СтрокаНачислений.Действие = Перечисления.ВидыДействияСНачислением.Прекратить;
				Если Выборка.ЗачетОтработанногоВремени Тогда
					НомерСтрокиОснНачисления = СтрокаНачислений.НомерСтроки;
				КонецЕсли;
			ИначеЕсли Выборка.ПНВидРасчета = NULL Тогда
				Если Выборка.ЗачетОтработанногоВремени И НомерСтрокиОснНачисления > 0 Тогда
					СтрокаНачислений.Действие = Перечисления.ВидыДействияСНачислением.Изменить;
					Начисления.Удалить(НомерСтрокиОснНачисления-1);
				Иначе
 					СтрокаНачислений.Действие = Перечисления.ВидыДействияСНачислением.Начать;
				КонецЕсли;
			Иначе
				СтрокаНачислений.Действие = Перечисления.ВидыДействияСНачислением.Изменить;
			КонецЕсли;
			Для Сч = 1 По 6 Цикл
				СтрокаНачислений["Валюта"+Сч] = Выборка["Валюта"+Сч];
				СтрокаНачислений["Показатель"+Сч] = Выборка["Показатель"+Сч];
				СтрокаНачислений["ТарифныйРазряд"+Сч] = Выборка["ТарифныйРазряд"+Сч];
			КонецЦикла;
			
		КонецЦикла;
	Иначе 
		Выборка = ЗапросПоСхемамМотивации.Выполнить().Выбрать();
		Взыскания.Очистить();
		Пока Выборка.Следующий() Цикл
			СтрокаВзыскания = Взыскания.Добавить();
			СтрокаВзыскания.Физлицо	= Выборка.Физлицо;
			СтрокаВзыскания.ВидРасчета = Выборка.ВидРасчета;
