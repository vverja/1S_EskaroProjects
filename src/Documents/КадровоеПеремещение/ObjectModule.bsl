			КонецЕсли;
			Для Сч = 1 По 6 Цикл
				СтрокаВзыскания["Валюта"+Сч] = Выборка["Валюта"+Сч];
				СтрокаВзыскания["Показатель"+Сч] = Выборка["Показатель"+Сч];
			КонецЦикла;
			
		КонецЦикла;

	КонецЕсли;
	
КонецПроцедуры

