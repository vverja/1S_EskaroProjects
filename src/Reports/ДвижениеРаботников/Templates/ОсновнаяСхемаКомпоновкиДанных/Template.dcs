		<field xsi:type="DataSetFieldField">
			<dataPath>ФизЛицоНеЯвляетсяНалоговымРезидентом</dataPath>
			<field>ФизЛицоНеЯвляетсяНалоговымРезидентом</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
