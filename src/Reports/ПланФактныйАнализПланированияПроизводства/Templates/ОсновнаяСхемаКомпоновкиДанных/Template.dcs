				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВложенныйЗапрос.ПериодДень КАК ПериодДень,
	ВложенныйЗапрос.Подразделение КАК Подразделение,
	ВложенныйЗапрос.Заказ КАК Заказ,
	ВложенныйЗапрос.Продукция КАК Продукция,
	ВложенныйЗапрос.ХарактеристикаПродукции КАК ХарактеристикаПродукции,
	ВложенныйЗапрос.КоличествоПлан КАК КоличествоПлан,
	0 КАК КоличествоЗадания,
	0 КАК КоличествоФакт
{ВЫБРАТЬ
	ПериодДень,
	Подразделение.*,
	Заказ.*,
	Продукция.*,
	ХарактеристикаПродукции.*,
	КоличествоПлан,
	КоличествоЗадания,
	КоличествоФакт}
ИЗ
	(ВЫБРАТЬ
		НАЧАЛОПЕРИОДА(План.ДатаОкончания, ДЕНЬ) КАК ПериодДень,
		План.РабочийЦентр.Подразделение КАК Подразделение,
		План.Заказ КАК Заказ,
		План.Номенклатура КАК Продукция,
		План.ХарактеристикаНоменклатуры КАК ХарактеристикаПродукции,
		СУММА(План.Количество) КАК КоличествоПлан
	ИЗ
		РегистрСведений.ДанныеДляПланированияЗанятостиРабочихЦентров КАК План
	ГДЕ
		План.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияРабочихЦентров.Занят)
		И ВЫБОР
				КОГДА &amp;НачалоПериода &lt;&gt; ДАТАВРЕМЯ(1, 1, 1)
					ТОГДА План.ДатаОкончания &gt;= НАЧАЛОПЕРИОДА(&amp;НачалоПериода, ДЕНЬ)
				ИНАЧЕ ИСТИНА
			КОНЕЦ
		И ВЫБОР
				КОГДА &amp;КонецПериода &lt;&gt; ДАТАВРЕМЯ(1, 1, 1)
					ТОГДА План.ДатаОкончания &lt;= КОНЕЦПЕРИОДА(&amp;КонецПериода, ДЕНЬ)
				ИНАЧЕ ИСТИНА
			КОНЕЦ
	{ГДЕ
		(НАЧАЛОПЕРИОДА(План.ДатаОкончания, ДЕНЬ)) КАК ПериодДень,
		План.РабочийЦентр.Подразделение.* КАК Подразделение,
		План.Заказ.* КАК Заказ,
		План.Номенклатура.* КАК Продукция,
		План.ХарактеристикаНоменклатуры.* КАК ХарактеристикаПродукции}
	
	СГРУППИРОВАТЬ ПО
		НАЧАЛОПЕРИОДА(План.ДатаОкончания, ДЕНЬ),
		План.РабочийЦентр.Подразделение,
		План.Заказ,
		План.Номенклатура,
		План.ХарактеристикаНоменклатуры
	
	ИМЕЮЩИЕ
		СУММА(План.Количество) &gt; 0) КАК ВложенныйЗапрос
{ГДЕ
	ВложенныйЗапрос.КоличествоПлан КАК КоличествоПлан}

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВложенныйЗапрос.ПериодДень,
	ВложенныйЗапрос.Подразделение,
	ВложенныйЗапрос.Заказ,
	ВложенныйЗапрос.Продукция,
	ВложенныйЗапрос.ХарактеристикаПродукции,
	0,
	ВложенныйЗапрос.КоличествоЗадания,
	0
ИЗ
	(ВЫБРАТЬ
		НАЧАЛОПЕРИОДА(План.ДатаОкончания, ДЕНЬ) КАК ПериодДень,
		План.РабочийЦентр.Подразделение КАК Подразделение,
		План.Заказ КАК Заказ,
		План.Номенклатура КАК Продукция,
		План.ХарактеристикаНоменклатуры КАК ХарактеристикаПродукции,
		СУММА(План.Количество) КАК КоличествоЗадания
	ИЗ
		РегистрСведений.ПланыЗанятостиРабочихЦентров КАК План
	ГДЕ
		План.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияРабочихЦентров.Занят)
		И ВЫБОР
				КОГДА &amp;НачалоПериода &lt;&gt; ДАТАВРЕМЯ(1, 1, 1)
					ТОГДА План.ДатаОкончания &gt;= НАЧАЛОПЕРИОДА(&amp;НачалоПериода, ДЕНЬ)
				ИНАЧЕ ИСТИНА
			КОНЕЦ
		И ВЫБОР
				КОГДА &amp;КонецПериода &lt;&gt; ДАТАВРЕМЯ(1, 1, 1)
					ТОГДА План.ДатаОкончания &lt;= КОНЕЦПЕРИОДА(&amp;КонецПериода, ДЕНЬ)
				ИНАЧЕ ИСТИНА
			КОНЕЦ
	{ГДЕ
		(НАЧАЛОПЕРИОДА(План.ДатаОкончания, ДЕНЬ)) КАК ПериодДень,
		План.РабочийЦентр.Подразделение.* КАК Подразделение,
		План.Заказ.* КАК Заказ,
		План.Номенклатура.* КАК Продукция,
		План.ХарактеристикаНоменклатуры.* КАК ХарактеристикаПродукции}
	
	СГРУППИРОВАТЬ ПО
		НАЧАЛОПЕРИОДА(План.ДатаОкончания, ДЕНЬ),
		План.РабочийЦентр.Подразделение,
		План.Заказ,
		План.Номенклатура,
		План.ХарактеристикаНоменклатуры
	
	ИМЕЮЩИЕ
		СУММА(План.Количество) &gt; 0) КАК ВложенныйЗапрос
{ГДЕ
	ВложенныйЗапрос.КоличествоЗадания КАК КоличествоЗадания}

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВложенныйЗапрос.ПериодДень,
	ВложенныйЗапрос.Подразделение,
	ВложенныйЗапрос.Заказ,
	ВложенныйЗапрос.Продукция,
	ВложенныйЗапрос.ХарактеристикаПродукции,
	0,
	0,
	ВложенныйЗапрос.КоличествоФакт
ИЗ
	(ВЫБРАТЬ
		НАЧАЛОПЕРИОДА(УчетЗатратОбороты.Период, ДЕНЬ) КАК ПериодДень,
		ВидУчета.Подразделение КАК Подразделение,
		УчетПартий.Заказ КАК Заказ,
		РаспределениеЗатрат.Продукция КАК Продукция,
		РаспределениеЗатрат.ХарактеристикаПродукции КАК ХарактеристикаПродукции,
		СУММА(УчетЗатратОбороты.КоличествоПриход) КАК КоличествоФакт
	ИЗ
		РегистрНакопления.УчетЗатрат.Обороты(&amp;НачалоПериода, &amp;КонецПериода, День, ) КАК УчетЗатратОбороты
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.АналитикаВидаУчета КАК ВидУчета
			ПО УчетЗатратОбороты.АналитикаВидаУчета = ВидУчета.Ссылка
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.АналитикаРаспределенияЗатрат КАК РаспределениеЗатрат
			ПО УчетЗатратОбороты.АналитикаРаспределенияЗатрат = РаспределениеЗатрат.Ссылка
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.АналитикаУчетаПартий КАК УчетПартий
			ПО УчетЗатратОбороты.АналитикаУчетаПартий = УчетПартий.Ссылка
	ГДЕ
		ВидУчета.РазделУчета = ЗНАЧЕНИЕ(Перечисление.РазделыУчета.Выпуск)
	{ГДЕ
		(НАЧАЛОПЕРИОДА(УчетЗатратОбороты.Период, ДЕНЬ)) КАК ПериодДень,
		ВидУчета.Подразделение.* КАК Подразделение,
		УчетПартий.Заказ.* КАК Заказ,
		РаспределениеЗатрат.Продукция.* КАК Продукция,
		РаспределениеЗатрат.ХарактеристикаПродукции.* КАК ХарактеристикаПродукции}
	
	СГРУППИРОВАТЬ ПО
		НАЧАЛОПЕРИОДА(УчетЗатратОбороты.Период, ДЕНЬ),
		ВидУчета.Подразделение,
		УчетПартий.Заказ,
		РаспределениеЗатрат.Продукция,
		РаспределениеЗатрат.ХарактеристикаПродукции) КАК ВложенныйЗапрос
{ГДЕ
	ВложенныйЗапрос.КоличествоФакт КАК КоличествоФакт}

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВложенныйЗапрос.ПериодДень,
	ВложенныйЗапрос.Подразделение,
	ВложенныйЗапрос.Заказ,
	ВложенныйЗапрос.Продукция,
	ВложенныйЗапрос.ХарактеристикаПродукции,
	0,
	0,
	ВложенныйЗапрос.КоличествоФакт
ИЗ
	(ВЫБРАТЬ
		НАЧАЛОПЕРИОДА(Выпуск.Период, ДЕНЬ) КАК ПериодДень,
		Выпуск.Подразделение КАК Подразделение,
		Выпуск.Заказ КАК Заказ,
		Выпуск.Продукция КАК Продукция,
		Выпуск.ХарактеристикаПродукции КАК ХарактеристикаПродукции,
		СУММА(Выпуск.КоличествоОборот) КАК КоличествоФакт
	ИЗ
		РегистрНакопления.ВыпускПродукции.Обороты(&amp;НачалоПериода, &amp;КонецПериода, День, {(Подразделение).* КАК Подразделение, (Заказ).* КАК Заказ, (Продукция).* КАК Продукция, (ХарактеристикаПродукции).* КАК ХарактеристикаПродукции}) КАК Выпуск
	
	СГРУППИРОВАТЬ ПО
		НАЧАЛОПЕРИОДА(Выпуск.Период, ДЕНЬ),
		Выпуск.Подразделение,
		Выпуск.Заказ,
		Выпуск.Продукция,
		Выпуск.ХарактеристикаПродукции) КАК ВложенныйЗапрос
{ГДЕ
	ВложенныйЗапрос.КоличествоФакт КАК КоличествоФакт}
{ХАРАКТЕРИСТИКИ
	ТИП(Справочник.ХарактеристикиНоменклатуры)
	СПИСОК (ВЫБРАТЬ
			СвойстваОбъектов.Ссылка,
			СвойстваОбъектов.Наименование,
			СвойстваОбъектов.ТипЗначения
		ИЗ
			ПланВидовХарактеристик.СвойстваОбъектов КАК СвойстваОбъектов
		ГДЕ
			СвойстваОбъектов.НазначениеСвойства = ЗНАЧЕНИЕ(ПланВидовХарактеристик.НазначенияСвойствКатегорийОбъектов.Справочник_ХарактеристикиНоменклатуры))
	ИДЕНТИФИКАТОР Ссылка
	ИМЯ Наименование
	ТИПЗНАЧЕНИЯ ТипЗначения
	ЗНАЧЕНИЯ РегистрСведений.ЗначенияСвойствОбъектов
	ОБЪЕКТ Объект
	ХАРАКТЕРИСТИКА Свойство
	ЗНАЧЕНИЕ Значение }
{ХАРАКТЕРИСТИКИ
	ТИП(Справочник.ХарактеристикиНоменклатуры)
	СПИСОК (ВЫБРАТЬ
			КатегорииОбъектов.Ссылка,
			КатегорииОбъектов.Наименование
		ИЗ
			Справочник.КатегорииОбъектов КАК КатегорииОбъектов
		ГДЕ
			КатегорииОбъектов.НазначениеКатегории = ЗНАЧЕНИЕ(ПланВидовХарактеристик.НазначенияСвойствКатегорийОбъектов.Справочник_ХарактеристикиНоменклатуры))
	ИДЕНТИФИКАТОР Ссылка
	ИМЯ Наименование
	ЗНАЧЕНИЯ РегистрСведений.КатегорииОбъектов
	ОБЪЕКТ Объект
	ХАРАКТЕРИСТИКА Категория }
{ХАРАКТЕРИСТИКИ
	ТИП(Справочник.КонтактныеЛицаКонтрагентов)
	СПИСОК (ВЫБРАТЬ
			СвойстваОбъектов.Ссылка,
			СвойстваОбъектов.Наименование,
			СвойстваОбъектов.ТипЗначения
		ИЗ
			ПланВидовХарактеристик.СвойстваОбъектов КАК СвойстваОбъектов
		ГДЕ
			СвойстваОбъектов.НазначениеСвойства = ЗНАЧЕНИЕ(ПланВидовХарактеристик.НазначенияСвойствКатегорийОбъектов.Справочник_КонтактныеЛицаКонтрагентов))
	ИДЕНТИФИКАТОР Ссылка
	ИМЯ Наименование
	ТИПЗНАЧЕНИЯ ТипЗначения
	ЗНАЧЕНИЯ РегистрСведений.ЗначенияСвойствОбъектов
	ОБЪЕКТ Объект
	ХАРАКТЕРИСТИКА Свойство
	ЗНАЧЕНИЕ Значение }
{ХАРАКТЕРИСТИКИ
	ТИП(Справочник.КонтактныеЛицаКонтрагентов)
	СПИСОК (ВЫБРАТЬ
			КатегорииОбъектов.Ссылка,
			КатегорииОбъектов.Наименование
		ИЗ
			Справочник.КатегорииОбъектов КАК КатегорииОбъектов
		ГДЕ
			КатегорииОбъектов.НазначениеКатегории = ЗНАЧЕНИЕ(ПланВидовХарактеристик.НазначенияСвойствКатегорийОбъектов.Справочник_КонтактныеЛицаКонтрагентов))
	ИДЕНТИФИКАТОР Ссылка
	ИМЯ Наименование
	ЗНАЧЕНИЯ РегистрСведений.КатегорииОбъектов
	ОБЪЕКТ Объект
	ХАРАКТЕРИСТИКА Категория }
{ХАРАКТЕРИСТИКИ
	ТИП(Справочник.КонтактныеЛица)
	СПИСОК (ВЫБРАТЬ
			СвойстваОбъектов.Ссылка,
			СвойстваОбъектов.Наименование,
			СвойстваОбъектов.ТипЗначения
		ИЗ
			ПланВидовХарактеристик.СвойстваОбъектов КАК СвойстваОбъектов
		ГДЕ
			СвойстваОбъектов.НазначениеСвойства = ЗНАЧЕНИЕ(ПланВидовХарактеристик.НазначенияСвойствКатегорийОбъектов.Справочник_КонтактныеЛица))
	ИДЕНТИФИКАТОР Ссылка
	ИМЯ Наименование
	ТИПЗНАЧЕНИЯ ТипЗначения
	ЗНАЧЕНИЯ РегистрСведений.ЗначенияСвойствОбъектов
	ОБЪЕКТ Объект
	ХАРАКТЕРИСТИКА Свойство
	ЗНАЧЕНИЕ Значение }
{ХАРАКТЕРИСТИКИ
	ТИП(Справочник.КонтактныеЛица)
