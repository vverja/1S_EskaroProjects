<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:annotation>
		<xs:documentation>
		Розрахунок частки використання сплаченого (нарахованого) податку за придбаними (ввезеними) товарами (послугами) між оподатковуваними та неоподатковуваними операціями
		[Д7 - Додаток 7 до податкової декларації з податку на додану вартість]
		(наказ ДПАУ від 25.01.2011 № 41)
		</xs:documentation>
	</xs:annotation>

     <xs:include schemaLocation="common_types.xsd" />

	<xs:element name="DECLAR" type="DeclarContent"/>
	<xs:complexType name="DeclarContent">
		<xs:sequence>
			<xs:element name="DECLARHEAD" type="DHead" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DECLARBODY" type="DBody" minOccurs="1" maxOccurs="1">
				<xs:unique name="UT2RXXXXG2"><xs:selector xpath="T2RXXXXG2"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT2RXXXXG3"><xs:selector xpath="T2RXXXXG3"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT2RXXXXG4"><xs:selector xpath="T2RXXXXG4"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT2RXXXXG5"><xs:selector xpath="T2RXXXXG5"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT2RXXXXG6"><xs:selector xpath="T2RXXXXG6"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT2RXXXXG7"><xs:selector xpath="T2RXXXXG7"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT2RXXXXG8"><xs:selector xpath="T2RXXXXG8"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT2RXXXXG9"><xs:selector xpath="T2RXXXXG9"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT2RXXXXG10"><xs:selector xpath="T2RXXXXG10"/><xs:field xpath="@ROWNUM"/></xs:unique>

				<xs:unique name="UT3RXXXXG2A"><xs:selector xpath="T3RXXXXG2A"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT3RXXXXG2B"><xs:selector xpath="T3RXXXXG2B"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT3RXXXXG3A"><xs:selector xpath="T3RXXXXG3A"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT3RXXXXG3B"><xs:selector xpath="T3RXXXXG3B"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT3RXXXXG4"><xs:selector xpath="T3RXXXXG4"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT3RXXXXG5"><xs:selector xpath="T3RXXXXG5"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT3RXXXXG6"><xs:selector xpath="T3RXXXXG6"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT3RXXXXG7"><xs:selector xpath="T3RXXXXG7"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT3RXXXXG8"><xs:selector xpath="T3RXXXXG8"/><xs:field xpath="@ROWNUM"/></xs:unique>

			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DHead">
		<xs:sequence>
			<xs:element name="TIN" type="DGLong" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC" type="xs:string" minOccurs="1" maxOccurs="1" fixed="F02"/>
			<xs:element name="C_DOC_SUB" type="xs:string" minOccurs="1" maxOccurs="1" fixed="157"/>
			<xs:element name="C_DOC_VER" type="xs:string" minOccurs="1" maxOccurs="1" fixed="1"/>
			<xs:element name="C_DOC_TYPE" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC_CNT" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_REG" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_RAJ" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_MONTH" type="DGMonth" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_TYPE" type="DGPType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_YEAR" type="DGYear" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_STI_ORIG" type="DGc_dpi" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC_STAN" type="DGSTAN" minOccurs="1" maxOccurs="1"/>
			<xs:element name="LINKED_DOCS" nillable="true" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="DOC" minOccurs="1" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="C_DOC" type="DGC_DOC" minOccurs="1" maxOccurs="1"/>
									<xs:element name="C_DOC_SUB" type="DGC_DOC_SUB" minOccurs="1" maxOccurs="1"/>
									<xs:element name="C_DOC_VER" type="DGC_DOC_VER" minOccurs="1" maxOccurs="1"/>
									<xs:element name="C_DOC_TYPE" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
									<xs:element name="C_DOC_CNT" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
									<xs:element name="C_DOC_STAN" type="DGSTAN" minOccurs="1" maxOccurs="1"/>
									<xs:element name="FILENAME" type="DGFilename" minOccurs="1" maxOccurs="1"/>
								</xs:sequence>
								<xs:attribute name="NUM" type="xs:nonNegativeInteger"/>
								<xs:attribute name="TYPE" type="DGTypeLinkDoc"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
				<xs:unique name="UH_DOC_NUM">
					<xs:selector xpath="DOC"/>
					<xs:field xpath="@NUM"/>
				</xs:unique>
			</xs:element>
			<xs:element name="D_FILL" type="DGDate" minOccurs="1" maxOccurs="1"/>
			<xs:element name="SOFTWARE" type="xs:string"  nillable="true" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DBody">
		<xs:sequence>
			<!--HEAD-->
			<xs:choice minOccurs="1" maxOccurs="1">
				<xs:element name="HZ" type="DGchk" />
				<xs:element name="HZN" type="DGchk" />
				<xs:element name="HZU" type="DGchk" />
			</xs:choice>
			
			<xs:element name="HZY" type="DGYear"	 minOccurs="1" maxOccurs="1"/>
			<xs:choice minOccurs="1" maxOccurs="1">
				<xs:element name="HZKV" type="DGKv" />
				<xs:element name="HZM" type="DGMonth" />
			</xs:choice>
			<xs:element name="HZYP" type="DGYear"	nillable="true" minOccurs="0" />
			<xs:choice>
				<xs:element name="HZKVP" type="DGKv" nillable="true" minOccurs="0" />
				<xs:element name="HZMP" type="DGMonth" nillable="true" minOccurs="0" />
			</xs:choice>

			<xs:element name="HCHV" type="DGdecimal2" nillable="true" minOccurs="0" />
			<xs:element name="HNAME" type="DGHNAME"/>

				<xs:element name="HTIN" type="DGHTINF"/>
			<xs:element name="HNPDV" type="DGHNPDV"/>
			<xs:element name="HNSPDV" type="DGHNSPDV"/>

			<!--BODY-->
			<xs:element name="R011G3" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R011G4" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R011G5" type="DGdecimal2" nillable="true" minOccurs="0"/>
			<xs:element name="R012G3" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R012G4" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R012G5" type="DGdecimal2" nillable="true" minOccurs="0"/>

			 <!--TABLE №1-->
			<xs:element name="T2RXXXXG2" type="Decimal0Column" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<xs:element name="T2RXXXXG3" type="Decimal0Column" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<xs:element name="T2RXXXXG4" type="Decimal0Column" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<xs:element name="T2RXXXXG5" type="Decimal2Column" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<xs:element name="T2RXXXXG6" type="Decimal0Column" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<xs:element name="T2RXXXXG7" type="Decimal0Column" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<xs:element name="T2RXXXXG8" type="Decimal0Column" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<xs:element name="T2RXXXXG9" type="Decimal0Column" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<xs:element name="T2RXXXXG10" type="Decimal0Column" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<!--/TABLE №1-->

			<!--TABLE №2-->
			<xs:element name="T3RXXXXG2A" type="MonthColumn" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<xs:element name="T3RXXXXG2B" type="YearColumn" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<xs:element name="T3RXXXXG3A" type="MonthColumn" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<xs:element name="T3RXXXXG3B" type="YearColumn" nillable="true" minOccurs="0" maxOccurs="999999"/>

			<xs:element name="T3RXXXXG4" type="Decimal2Column" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<xs:element name="T3RXXXXG5" type="Decimal2Column" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<xs:element name="T3RXXXXG6" type="Decimal2Column" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<xs:element name="T3RXXXXG7" type="Decimal2Column" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<xs:element name="T3RXXXXG8" type="Decimal2Column" nillable="true" minOccurs="0" maxOccurs="999999"/>
			<!--/TABLE №2-->

			<xs:element name="R041G1" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R041G2" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R041G3" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R041G4" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R041G5" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R041G6" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R041G7" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R041G8" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R041G9" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R041G10" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R041G11" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R041G12" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R041G13" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R041G14" type="DGdecimal0" nillable="true" minOccurs="0"/>

			<!--FOOTER-->
			<xs:element name="HFILL" type="DGDate"	minOccurs="1" maxOccurs="1"/>

			<xs:element name="HBUH" type="DGHBUH"	nillable="true" minOccurs="0"/>
			<xs:element name="HFO" type="DGHFO"   minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
