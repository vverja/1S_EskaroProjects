<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:annotation>
		<xs:documentation>Довідка до ДЕКЛАРАЦІЇ З ПОДАТКУ НА ДОДАНУ ВАРТІСТЬ
		(ЗАТВЕРДЖЕНО Наказ ДПА України 30.05.97 № 166 (у редакції наказу ДПА України від 15.06.2005 № 213))</xs:documentation>
	</xs:annotation>

 <xs:include schemaLocation="comtypes.xsd" />

	<xs:element name="DECLAR" type="DeclarContent"/>
	<xs:complexType name="DeclarContent">
		<xs:sequence>
			<xs:element name="DECLARHEAD" type="DHead" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DECLARBODY" type="DBody" minOccurs="1" maxOccurs="1">
				<xs:unique name="UT1RXXXXG2S"><xs:selector xpath="T1RXXXXG2S"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT1RXXXXG3S"><xs:selector xpath="T1RXXXXG3S"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT1RXXXXG4"><xs:selector xpath="T1RXXXXG4"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT2RXXXXG2S"><xs:selector xpath="T2RXXXXG2S"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT2RXXXXG3S"><xs:selector xpath="T2RXXXXG3S"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT2RXXXXG4"><xs:selector xpath="T2RXXXXG4"/><xs:field xpath="@ROWNUM"/></xs:unique>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DHead">
		<xs:sequence>
			<xs:element name="TIN" type="DGLong" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC" type="xs:string" minOccurs="1" maxOccurs="1" fixed="J02"/>
			<xs:element name="C_DOC_SUB" type="xs:string" minOccurs="1" maxOccurs="1" fixed="993"/>
			<xs:element name="C_DOC_VER" type="xs:string" minOccurs="1" maxOccurs="1" fixed="1"/>
			<xs:element name="C_DOC_TYPE" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC_CNT" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_REG" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_RAJ" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_MONTH" type="DGMonth" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_TYPE" type="DGPType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_YEAR" type="DGYear" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC_STAN" type="DGSTAN" minOccurs="1" maxOccurs="1"/>
			<xs:element name="D_FILL" type="DGDate" minOccurs="1" maxOccurs="1"/>
			<xs:element name="SOFTWARE" type="xs:string"  nillable="true" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DBody">
		<xs:sequence>
			<!--HEAD-->

			<xs:choice minOccurs="1" maxOccurs="1">
      <xs:element name="HZ" type="DGchk" />
      <xs:element name="HZN" type="DGchk" />
      <xs:element name="HZU" type="DGchk" />
			</xs:choice>

	 <xs:element name="HZY" type="DGYear" minOccurs="1" maxOccurs="1"/>
			<xs:choice minOccurs="1" maxOccurs="1">
      <xs:element name="HZKV" type="DGKv" />
      <xs:element name="HZM" type="DGMonth"  />
			</xs:choice>

	<xs:element name="HZYP" type="DGYear"  nillable="true" minOccurs="0"  />
			<xs:choice>
      <xs:element name="HZKVP" type="DGKv" nillable="true" minOccurs="0"  />
      <xs:element name="HZMP" type="DGMonth" nillable="true" minOccurs="0"  />
			</xs:choice>

       <xs:element name="HNAME" type="DGHNAME" minOccurs="1" maxOccurs="1"/>

			<xs:choice minOccurs="1" maxOccurs="1">
      <xs:element name="HTINJ" type="DGHTINJ"  />
      <xs:element name="HTINF" type="DGHTINF"  />
      <xs:element name="HTINSD" type="DGHTINSD"   />
      <xs:element name="HTINNR" type="DGHTINNR"   />
			</xs:choice>

      <xs:element name="HDDGVSD" type="DGHDDGVSD"  nillable="true" minOccurs="0"  />
      <xs:element name="HNDGVSD" type="DGHNDGVSD"  nillable="true" minOccurs="0"  />

      <xs:element name="HNPDV" type="DGHNPDV"  minOccurs="1" maxOccurs="1"/>
      <xs:element name="HNSPDV" type="DGHNSPDV" minOccurs="1" maxOccurs="1"/>
      <xs:element name="HLOC" type="DGHLOC" minOccurs="1" maxOccurs="1"/>

      <xs:element name="HZIP" type="DGHZIP" nillable="true" minOccurs="0"  />
      <xs:element name="HTEL" type="DGHTEL"  nillable="true" minOccurs="0"  />
      <xs:element name="HFAX" type="DGHFAX"  nillable="true" minOccurs="0"  />
      <xs:element name="HEMAIL" type="DGHEMAIL"  nillable="true" minOccurs="0"	/>
      <xs:element name="HSTI" type="DGHSTI" minOccurs="1" maxOccurs="1"/>

      <xs:element name="R00G07S" type="DGchk" nillable="true" minOccurs="0"  />
      <xs:element name="R00G08S" type="DGchk" nillable="true" minOccurs="0"  />
      <xs:element name="R00G09S" type="DGchk" nillable="true" minOccurs="0"  />
      <xs:element name="R00G10S" type="DGchk" nillable="true" minOccurs="0"  />

			<!--BODY-->
	 <xs:element name="T1RXXXXG2S" type="StrColumn"  nillable="true" minOccurs="0" maxOccurs="99"/>
	 <xs:element name="T1RXXXXG3S" type="StrColumn"  nillable="true" minOccurs="0" maxOccurs="99"/>
	 <xs:element name="T1RXXXXG4" type="Decimal0Column"  nillable="true" minOccurs="0" maxOccurs="99"/>

	 <xs:element name="R01G4" type="DGdecimal0"  nillable="true" minOccurs="0" maxOccurs="99"/>
			   <!--TABLE № 2-->
	 <xs:element name="T2RXXXXG2S" type="StrColumn"  nillable="true" minOccurs="0" maxOccurs="99"/>
	 <xs:element name="T2RXXXXG3S" type="StrColumn"  nillable="true" minOccurs="0" maxOccurs="99"/>
	 <xs:element name="T2RXXXXG4" type="Decimal0Column"  nillable="true" minOccurs="0" maxOccurs="99"/>

	 <xs:element name="R02G4" type="DGdecimal0"  nillable="true" minOccurs="0" maxOccurs="99"/>
			<!--FOOTER-->
			<xs:element name="HFILL" type="DGDate" minOccurs="1" maxOccurs="1"/>
			<xs:element name="HBOS" type="DGHBOS"  minOccurs="1" maxOccurs="1"/>
			<xs:element name="HBUH" type="DGHBUH"  nillable="true" minOccurs="0"  />
			<xs:element name="HFO" type="DGHFO"  nillable="true" minOccurs="0"  />
		</xs:sequence>
	</xs:complexType>
</xs:schema>
