<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:annotation>
		<xs:documentation>РОЗРАХУНОК КОРИГУВАННЯ СУМ ПДВ до податкової декларації з податку на додану вартість(скороченої) (Додаток 2. до декларації з ПДВ, (у редакції наказу ДПА України від 2 листопада 2000 р. N 568))</xs:documentation>
	</xs:annotation>

	<xs:simpleType name="DGMonth">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="12"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="DGYear">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1991"/>
			<xs:maxInclusive value="2099"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="DGPType">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="5"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="DGsti">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="99"/>
		</xs:restriction>
	</xs:simpleType>

<xs:simpleType name="DGLong">
		<xs:restriction base="xs:unsignedLong">
			<xs:minInclusive value="1000"/>
			<xs:maxInclusive value="9999999999"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="DGDate">
		<xs:restriction base="xs:string">
			<xs:length value="8"/>
			<xs:pattern value="(((((0[1-9]|[1-2][0-9])(0(1|[3-9])|1[0-2]))|(30(0(1|[3-9])|1[0-2]))|(31(01|03|05|07|08|10|12)))(19|20)\d{2})|((0[1-9]|[1-2][0-9])02(19|20)[0-9](0|4))|((0[1-9]|[1-2][0-8]|19)02(19|20)[0-9](1|2|3|5|6|7|8|9)))"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="DGKv">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="4"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="rowInt">
		<xs:restriction base="xs:int">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="DECLAR" type="DeclarContent"/>
	<xs:complexType name="DeclarContent">
		<xs:sequence>
			<xs:element name="DECLARHEAD" type="DHead" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DECLARBODY" type="DBody" minOccurs="1" maxOccurs="1">
				<xs:unique name="UT1RXXXXG2S"><xs:selector xpath="T1RXXXXG2S"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT1RXXXXG3"><xs:selector xpath="T1RXXXXG3"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT1RXXXXG4"><xs:selector xpath="T1RXXXXG4"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT2RXXXXG2S"><xs:selector xpath="T2RXXXXG2S"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT2RXXXXG3"><xs:selector xpath="T2RXXXXG3"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UT2RXXXXG4"><xs:selector xpath="T2RXXXXG4"/><xs:field xpath="@ROWNUM"/></xs:unique>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DecimalColumn">
		<xs:simpleContent>
			<xs:extension base="xs:decimal">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="StrColumn">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="DHead">
		<xs:sequence>
			<xs:element name="TIN" type="DGLong" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC" type="xs:string" minOccurs="1" maxOccurs="1" fixed="J02"/>
			<xs:element name="C_DOC_SUB" type="xs:string" minOccurs="1" maxOccurs="1" fixed="045"/>
			<xs:element name="C_DOC_VER" type="xs:string" minOccurs="1" maxOccurs="1" fixed="2"/>
			<xs:element name="C_DOC_TYPE" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC_CNT" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_REG" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_RAJ" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_MONTH" type="DGMonth" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_TYPE" type="DGPType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_YEAR" type="DGYear" minOccurs="1" maxOccurs="1"/>
			<xs:element name="D_FILL" type="DGDate" minOccurs="1" maxOccurs="1"/>
			<xs:element name="SOFTWARE" type="xs:string"  nillable="true" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DBody">
		<xs:sequence>
			<!--HEAD-->
  		<xs:element name="HZY" type="DGYear"  nillable="true" minOccurs="0" maxOccurs="1"/>
  		<xs:element name="HZKV" type="DGKv"  nillable="true" minOccurs="0" maxOccurs="1"/>
  		<xs:element name="HZM" type="DGMonth"  nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="HNAME" type="xs:string"  nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="HNPDV" type="xs:string"  nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="HNSPDV" type="xs:string"  nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="HLOC" type="xs:string"  nillable="true" minOccurs="0" maxOccurs="1"/>
      <xs:element name="HTEL" type="xs:string"  nillable="true" minOccurs="0" maxOccurs="1"/>
			<!--BODY-->
			<!--TABLE №1-->
    	<xs:sequence minOccurs="0" maxOccurs="999">
 			<xs:element name="T1RXXXXG2S" type="StrColumn"  nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="T1RXXXXG3" type="DecimalColumn"  nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="T1RXXXXG4" type="DecimalColumn"  nillable="true" minOccurs="0" maxOccurs="1"/>
    	</xs:sequence>

			<xs:element name="R06GA" type="xs:decimal"  nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="R07GB" type="xs:decimal"  nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="R80GB" type="xs:decimal"  nillable="true" minOccurs="0" maxOccurs="1"/>
			<!--/TABLE №1-->

			<!--TABLE №2-->
    	<xs:sequence minOccurs="0" maxOccurs="999">
 			<xs:element name="T2RXXXXG2S" type="StrColumn"  nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="T2RXXXXG3" type="DecimalColumn"  nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="T2RXXXXG4" type="DecimalColumn"  nillable="true" minOccurs="0" maxOccurs="1"/>
    	</xs:sequence>

			<xs:element name="R13GA" type="xs:decimal"  nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="R14GB" type="xs:decimal"  nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="R16GB" type="xs:decimal"  nillable="true" minOccurs="0" maxOccurs="1"/>
			<!--/TABLE №2-->
			<!--FOOTER-->
			<xs:element name="HFILL" type="DGDate"  nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HBOS" type="xs:string"  nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HBUH" type="xs:string"  nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HFO" type="xs:string"  nillable="true" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
