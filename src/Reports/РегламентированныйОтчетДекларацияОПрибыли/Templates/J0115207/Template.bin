<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:annotation>
		<xs:documentation>Додаток К1/2 до рядків 04.10 та 07 декларації З ПОДАТКУ НА ПРИБУТОК(ЗАТВЕРДЖЕНО наказом ДПАУ від 29 березня 2003 р. N 143)</xs:documentation>
	</xs:annotation>
	<xs:simpleType name="DGMonth">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="12"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DGYear">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1991"/>
			<xs:maxInclusive value="2099"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DGsti">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="99"/>
		</xs:restriction>
	</xs:simpleType>

<xs:simpleType name="DGLong">
		<xs:restriction base="xs:unsignedLong">
			<xs:minInclusive value="1000"/>
			<xs:maxInclusive value="9999999999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DGDate">
		<xs:restriction base="xs:string">
		<xs:length value="8"/>
		<xs:pattern value="(((((0[1-9]|[1-2][0-9])(0(1|[3-9])|1[0-2]))|(30(0(1|[3-9])|1[0-2]))|(31(01|03|05|07|08|10|12)))(19|20)\d{2})|((0[1-9]|[1-2][0-9])02(19|20)[0-9](0|4))|((0[1-9]|[1-2][0-8]|19)02(19|20)[0-9](1|2|3|5|6|7|8|9)))"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DGchk">
    <xs:restriction base="xs:int">
      <xs:enumeration value="1"/>
    </xs:restriction>
  </xs:simpleType>
	<xs:simpleType name="rowInt">
		<xs:restriction base="xs:int">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="DecimalColumn">
		<xs:simpleContent>
			<xs:extension base="xs:decimal">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="StrColumn">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:element name="DECLAR" type="DeclarContent"/>
	<xs:complexType name="DeclarContent">
		<xs:sequence>
			<xs:element name="DECLARHEAD" type="DHead" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DECLARBODY" type="DBody" minOccurs="1" maxOccurs="1">
				<xs:unique name="UK12T1RXXXXG2S"><xs:selector xpath="K12T1RXXXXG2S"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UK12T1RXXXXG3"><xs:selector xpath="K12T1RXXXXG3"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UK12T1RXXXXG4"><xs:selector xpath="K12T1RXXXXG4"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UK12T2RXXXXG2S"><xs:selector xpath="K12T2RXXXXG2S"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UK12T2RXXXXG3"><xs:selector xpath="K12T2RXXXXG3"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UK12T2RXXXXG4"><xs:selector xpath="K12T2RXXXXG4"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UK12T2RXXXXG5"><xs:selector xpath="K12T2RXXXXG5"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UK12T2RXXXXG6"><xs:selector xpath="K12T2RXXXXG6"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UK12T2RXXXXG7"><xs:selector xpath="K12T2RXXXXG7"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UK12T3RXXXXG2S"><xs:selector xpath="K12T3RXXXXG2S"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UK12T3RXXXXG3"><xs:selector xpath="K12T3RXXXXG3"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UK12T3RXXXXG4"><xs:selector xpath="K12T3RXXXXG4"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UK12T3RXXXXG5"><xs:selector xpath="K12T3RXXXXG5"/><xs:field xpath="@ROWNUM"/></xs:unique>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DHead">
		<xs:sequence>
			<xs:element name="TIN" type="DGLong" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC" type="xs:string" minOccurs="1" maxOccurs="1" fixed="J01"/>
			<xs:element name="C_DOC_SUB" type="xs:string" minOccurs="1" maxOccurs="1" fixed="152"/>
			<xs:element name="C_DOC_VER" type="xs:string" minOccurs="1" maxOccurs="1" fixed="7"/>
			<xs:element name="C_DOC_TYPE" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC_CNT" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_REG" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_RAJ" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_MONTH" type="DGMonth" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_YEAR" type="DGYear" minOccurs="1" maxOccurs="1"/>
			<xs:element name="D_FILL" type="DGDate" minOccurs="1" maxOccurs="1"/>
			<xs:element name="SOFTWARE" type="xs:string" nillable="true" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DBody">
		<xs:sequence>
			<!--header-->
			<xs:element name="HZY" type="DGYear" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HTIN" type="DGLong" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="H1KV" type="DGchk" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HHY" type="DGchk" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="H3KV" type="DGchk" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HY" type="DGchk" nillable="true" minOccurs="0" maxOccurs="1"/>
			<!--body-->
			<!--TABLE №1-->
			<xs:element name="K12R1G4" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K12R2G4" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>

    	<xs:sequence minOccurs="0" maxOccurs="999">
			<xs:element name="K12T1RXXXXG2S" type="StrColumn" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K12T1RXXXXG3" type="DecimalColumn" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K12T1RXXXXG4" type="DecimalColumn" nillable="true" minOccurs="0" maxOccurs="1"/>
    	</xs:sequence>

			<xs:element name="K12R7G4" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<!--/Table №1-->
			<!--Table2 №2-->
    	<xs:sequence minOccurs="0" maxOccurs="999">
			<xs:element name="K12T2RXXXXG2S" type="StrColumn" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K12T2RXXXXG3" type="DecimalColumn" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K12T2RXXXXG4" type="DecimalColumn" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K12T2RXXXXG5" type="DecimalColumn" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K12T2RXXXXG6" type="DecimalColumn" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K12T2RXXXXG7" type="DecimalColumn" nillable="true" minOccurs="0" maxOccurs="1"/>
    	</xs:sequence>

	 		<xs:element name="K12R410G4" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
	 		<xs:element name="K12R410G5" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
	 		<xs:element name="K12R410G6" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
	 		<xs:element name="K12R410G7" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<!--/Table №2-->
			<!--Table №3-->
    	<xs:sequence minOccurs="0" maxOccurs="999">
			<xs:element name="K12T3RXXXXG2S" type="StrColumn" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K12T3RXXXXG3" type="DecimalColumn" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K12T3RXXXXG4" type="DecimalColumn" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K12T3RXXXXG5" type="DecimalColumn" nillable="true" minOccurs="0" maxOccurs="1"/>
    	</xs:sequence>
			<!--/Table №3-->
			<!--footer-->
			<xs:element name="HBOS" type="xs:string" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HBUH" type="xs:string" nillable="true" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
