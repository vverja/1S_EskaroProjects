<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:annotation>
		<xs:documentation>Додаток К2 до рядків 01.3 та 04.7 декларації З ПОДАТКУ НА ПРИБУТОК(ЗАТВЕРДЖЕНО наказом ДПАУ від 29 березня 2003 р. N 143)</xs:documentation>
	</xs:annotation>
	<xs:simpleType name="DGMonth">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="12"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DGYear">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1991"/>
			<xs:maxInclusive value="2099"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DGsti">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="99"/>
		</xs:restriction>
	</xs:simpleType>

<xs:simpleType name="DGLong">
		<xs:restriction base="xs:unsignedLong">
			<xs:minInclusive value="1000"/>
			<xs:maxInclusive value="9999999999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DGDate">
		<xs:restriction base="xs:string">
		<xs:length value="8"/>
		<xs:pattern value="(((((0[1-9]|[1-2][0-9])(0(1|[3-9])|1[0-2]))|(30(0(1|[3-9])|1[0-2]))|(31(01|03|05|07|08|10|12)))(19|20)\d{2})|((0[1-9]|[1-2][0-9])02(19|20)[0-9](0|4))|((0[1-9]|[1-2][0-8]|19)02(19|20)[0-9](1|2|3|5|6|7|8|9)))"/>
		</xs:restriction>
	</xs:simpleType>
 	<xs:simpleType name="DGchk">
    <xs:restriction base="xs:int">
      <xs:enumeration value="1"/>
    </xs:restriction>
  </xs:simpleType>

	<xs:element name="DECLAR" type="DeclarContent"/>
	<xs:complexType name="DeclarContent">
		<xs:sequence>
			<xs:element name="DECLARHEAD" type="DHead" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DECLARBODY" type="DBody" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DHead">
		<xs:sequence>
			<xs:element name="TIN" type="DGLong" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC" type="xs:string" minOccurs="1" maxOccurs="1" fixed="J01"/>
			<xs:element name="C_DOC_SUB" type="xs:string" minOccurs="1" maxOccurs="1" fixed="153"/>
			<xs:element name="C_DOC_VER" type="xs:string" minOccurs="1" maxOccurs="1" fixed="7"/>
			<xs:element name="C_DOC_TYPE" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC_CNT" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_REG" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_RAJ" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_MONTH" type="DGMonth" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_YEAR" type="DGYear" minOccurs="1" maxOccurs="1"/>
			<xs:element name="D_FILL" type="DGDate" minOccurs="1" maxOccurs="1"/>
			<xs:element name="SOFTWARE" type="xs:string" nillable="true" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DBody">
		<xs:sequence>
			<!--header-->
			<xs:element name="HZY" type="DGYear" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HTIN" type="DGLong" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="H1KV" type="DGchk" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HHY" type="DGchk" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="H3KV" type="DGchk" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HY" type="DGchk" nillable="true" minOccurs="0" maxOccurs="1"/>
			<!--body-->
			<xs:element name="K2R1G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R1G4" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R1G5" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R1G6" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>

			<xs:element name="K2R2G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R2G4" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R2G5" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R2G6" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>

			<xs:element name="K2R3G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R3G4" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R3G5" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R3G6" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>

			<xs:element name="K2R4G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R4G4" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R4G5" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R4G6" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>

			<xs:element name="K2R5G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R5G4" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R5G5" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R5G6" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>

			<xs:element name="K2R6G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R6G4" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R6G5" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R6G6" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>

			<xs:element name="K2R7G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R7G4" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R7G5" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R7G6" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>

			<xs:element name="K2R8G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R8G4" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R8G5" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="K2R8G6" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<!--footer-->
			<xs:element name="HBOS" type="xs:string" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HBUH" type="xs:string" nillable="true" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
