			<xs:element name="DECLARBODY" type="DBody" minOccurs="1" maxOccurs="1">
				<xs:unique name="UK6T2RXXXXG3S"><xs:selector xpath="K6T2RXXXXG3S"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UK6T2RXXXXG3"><xs:selector xpath="K6T2RXXXXG3"/><xs:field xpath="@ROWNUM"/></xs:unique>
