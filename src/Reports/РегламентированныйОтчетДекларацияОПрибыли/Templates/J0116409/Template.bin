<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:annotation>
		<xs:documentation>Додаток Р3 до рядка 04.11 ДЕКЛАРАЦІІ З ПОДАТКУ НА ПРИБУТОК ПІДПРИЄМСТВА(ЗАТВЕРДЖЕНО наказом ДПАУ від 29 березня 2003 р. N 143
(зі змінами згідно наказу ДПА України від 12.10.2005 № 448))</xs:documentation>
	</xs:annotation>

<xs:include schemaLocation="common_types.xsd" />

	<xs:element name="DECLAR" type="DeclarContent"/>
	<xs:complexType name="DeclarContent">
		<xs:sequence>
			<xs:element name="DECLARHEAD" type="DHead" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DECLARBODY" type="DBody" minOccurs="1" maxOccurs="1">
				<xs:unique name="UP4T1RXXXXG2S"><xs:selector xpath="P4T1RXXXXG2S"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UP4T1RXXXXG3S"><xs:selector xpath="P4T1RXXXXG3S"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UP4T1RXXXXG4S"><xs:selector xpath="P4T1RXXXXG4S"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UP4T1RXXXXG5"><xs:selector xpath="P4T1RXXXXG5"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="UP4T1RXXXXG6"><xs:selector xpath="P4T1RXXXXG6"/><xs:field xpath="@ROWNUM"/></xs:unique>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DHead">
		<xs:sequence>
			<xs:element name="TIN" type="DGLong" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC" type="xs:string" minOccurs="1" maxOccurs="1" fixed="J01"/>
			<xs:element name="C_DOC_SUB" type="xs:string" minOccurs="1" maxOccurs="1" fixed="164"/>
			<xs:element name="C_DOC_VER" type="xs:string" minOccurs="1" maxOccurs="1" fixed="9"/>
			<xs:element name="C_DOC_TYPE" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC_CNT" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_REG" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_RAJ" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_MONTH" type="DGMonth" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_TYPE" type="DGPType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_YEAR" type="DGYear" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC_STAN" type="DGSTAN" minOccurs="1" maxOccurs="1"/>
			<xs:element name="D_FILL" type="DGDate" minOccurs="1" maxOccurs="1"/>
			<xs:element name="SOFTWARE" type="xs:string" nillable="true" minOccurs="0"  />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DBody">
		<xs:sequence>
			<!--HEAD-->
      <xs:element name="HTIN" type="DGHTINJ" minOccurs="1" maxOccurs="1"/>
      <xs:element name="HZY" type="DGYear" minOccurs="1" maxOccurs="1"/>
			<!--BODY-->
			<!--TABLE-->

			<xs:element name="P4T1RXXXXG2S" type="StrColumn" nillable="true" minOccurs="0" maxOccurs="999"/>
			<xs:element name="P4T1RXXXXG3S" type="StrColumn" nillable="true" minOccurs="0" maxOccurs="999"/>
			<xs:element name="P4T1RXXXXG4S" type="StrColumn" nillable="true" minOccurs="0" maxOccurs="999"/>
			<xs:element name="P4T1RXXXXG5" type="Decimal0Column" nillable="true" minOccurs="0" maxOccurs="999"/>
			<xs:element name="P4T1RXXXXG6" type="Decimal0Column" nillable="true" minOccurs="0" maxOccurs="999"/>

			<xs:element name="P4R411G6" type="DGdecimal0" nillable="true" minOccurs="0"  />
			<!--/TABLE-->
			<!--FOOTER-->
			<xs:element name="HBOS" type="DGHBOS" minOccurs="1" maxOccurs="1"/>
			<xs:element name="HBUH" type="DGHBUH" nillable="true" minOccurs="0"  />
		</xs:sequence>
	</xs:complexType>
</xs:schema>
