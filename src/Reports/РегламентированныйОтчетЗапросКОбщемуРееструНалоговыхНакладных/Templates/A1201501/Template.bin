<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:annotation>
		<xs:documentation>Витяг із Загального реєстру податкових накладних  </xs:documentation>
	</xs:annotation>

  <xs:include schemaLocation="common_types.xsd" />

	<xs:element name="DECLAR" type="DeclarContent"/>
	<xs:complexType name="DeclarContent">
		<xs:sequence>
			<xs:element name="DECLARHEAD" type="DHead" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DECLARBODY" type="DBody" minOccurs="1" maxOccurs="1">
       <xs:unique name="UT1RXXXXG2D">  <xs:selector xpath="T1RXXXXG1D"/>  <xs:field xpath="@ROWNUM"/>  </xs:unique>
       <xs:unique name="UT1RXXXXG3S">  <xs:selector xpath="T1RXXXXG3S"/>  <xs:field xpath="@ROWNUM"/>  </xs:unique>
       <xs:unique name="UT1RXXXXG4D">  <xs:selector xpath="T1RXXXXG4D"/>  <xs:field xpath="@ROWNUM"/>  </xs:unique>
       <xs:unique name="UT1RXXXXG5S">  <xs:selector xpath="T1RXXXXG5S"/>  <xs:field xpath="@ROWNUM"/>  </xs:unique>
       <xs:unique name="UT1RXXXXG6">   <xs:selector xpath="T1RXXXXG6"/>   <xs:field xpath="@ROWNUM"/>  </xs:unique>
       <xs:unique name="UT1RXXXXG7S">  <xs:selector xpath="T1RXXXXG7S"/>  <xs:field xpath="@ROWNUM"/>  </xs:unique>
       <xs:unique name="UT1RXXXXG8D">  <xs:selector xpath="T1RXXXXG8D"/>  <xs:field xpath="@ROWNUM"/>  </xs:unique>
       <xs:unique name="UT1RXXXXG9">   <xs:selector xpath="T1RXXXXG9"/>   <xs:field xpath="@ROWNUM"/>  </xs:unique>

			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DHead">
		<xs:sequence>
			<xs:element name="TIN" type="DGLong" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC" type="xs:string" minOccurs="1" maxOccurs="1" fixed="A12"/>
			<xs:element name="C_DOC_SUB" type="xs:string" minOccurs="1" maxOccurs="1" fixed="015"/>
			<xs:element name="C_DOC_VER" type="xs:string" minOccurs="1" maxOccurs="1" fixed="1"/>
			<xs:element name="C_DOC_TYPE" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC_CNT" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_REG" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_RAJ" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_MONTH" type="DGMonth" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_TYPE" type="DGPType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_YEAR" type="DGYear" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC_STAN" type="DGSTAN" minOccurs="1" maxOccurs="1"/>
			<xs:element name="D_FILL" type="DGDate" minOccurs="1" maxOccurs="1"/>
			<xs:element name="SOFTWARE" type="xs:string" nillable="true" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DBody">
		<xs:sequence>
			<!--HEAD-->

			<xs:element name="HNUM" type="xs:string" nillable="true" minOccurs="0" />
			<xs:element name="HDATE" type="DGDate" nillable="true" minOccurs="0" />
			<xs:element name="HNUM1" type="xs:string" nillable="true" minOccurs="0" />
			<xs:element name="HTIN" type="DGLong" nillable="true" minOccurs="0" />
			<xs:element name="HKSEL" type="DGHNPDV" nillable="true" minOccurs="0" />
			<xs:element name="HSTI" type="DGHSTI" nillable="true" minOccurs="0" />
			<xs:element name="HEMAIL" type="DGHEMAIL" nillable="true" minOccurs="0" />
			<!--BODY-->
			<!--TABLE №1-->

				<xs:element name="T1RXXXXG2D" type="DateColumn" nillable="true" minOccurs="0" maxOccurs="999999"/>
				<xs:element name="T1RXXXXG3S" type="StrColumn" nillable="true" minOccurs="0" maxOccurs="999999"/>
				<xs:element name="T1RXXXXG4D" type="DateColumn" nillable="true" minOccurs="0" maxOccurs="999999"/>
				<xs:element name="T1RXXXXG5S" type="IndTaxNumColumn" nillable="true" minOccurs="0" maxOccurs="999999"/>
				<xs:element name="T1RXXXXG6" type="Decimal2Column" nillable="true" minOccurs="0" maxOccurs="999999"/>
				<xs:element name="T1RXXXXG7S" type="StrColumn" nillable="true" minOccurs="0" maxOccurs="999999"/>
				<xs:element name="T1RXXXXG8D" type="DateColumn" nillable="true" minOccurs="0" maxOccurs="999999"/>
				<xs:element name="T1RXXXXG9" type="Decimal2Column" nillable="true" minOccurs="0" maxOccurs="999999"/>

			<!--/TABLE №1-->

		       <xs:element name="HFILL" type="DGDate" nillable="true" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
</xs:schema>
