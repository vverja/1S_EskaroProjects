	<xs:simpleType name="DGHTINSD">
		<xs:restriction base="DGLong"/>
	</xs:simpleType>
	<!--Загальний тип "Код HTINNR (Ідентифікаційний код представництвa нерезидента, що не має статусу юридичної особи)"-->
	<xs:simpleType name="DGHTINNR">
		<xs:restriction base="DGNLong"/>
	</xs:simpleType>

		<!--Загальний тип "Індивідуальний податковий номер "-->
	<xs:simpleType name="DGLong12">
		<xs:restriction base="xs:unsignedLong">
			<xs:minInclusive value="1000"/>
			<xs:maxInclusive value="999999999999"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код HNPDV (Індивідуальний податковий номер платника ПДВ)"-->
	<xs:simpleType name="DGHNPDV">
		<xs:restriction base="DGLong12"/>
	</xs:simpleType>

	<!--Загальний тип "Індивідуальний податковий номер платника"-->
	<xs:simpleType name="DGHIPN">
		<xs:restriction base="DGLong12"/>
	</xs:simpleType>
	<!--Загальний тип "Індивідуальний податковий номер платника з можливістю внесення 0(нуля)"-->
	<xs:simpleType name="DGHIPN0"> <!-- 17.02.2011 модифіковано відповідно до листа від 17.02.11 № 28-0517/61 -->
		<xs:annotation><xs:documentation>
			Відповідно до Положення про реєстрацію платників податку на додану вартість, п.6.3 наказ ДПАУ від 22.12.2010 № 978
			Індивідуальний податковий номер платника, можливі значення:
			* 0 (нуль)
			* число від 6 до 12 чисел 
			* № паспорта
		</xs:documentation></xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(0)|([0-9]{6,12})|([АБВГДЕЄЖЗИІКЛМНОПРСТУФХЦЧШЩЮЯ]{2}[0-9]{6})"/>
		</xs:restriction>
	</xs:simpleType>

	<!--Загальний тип "Код HNSPDV (номер свідоцтва про реєстрацію платника ПДВ)"-->
	<xs:simpleType name="DGHNSPDV"> <!-- 30.12.2010 Внесено можливість вносити 0 відповідно до п.8 наказу ДПАУ від 21.12.2010 N969 -->
