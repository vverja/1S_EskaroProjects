<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:annotation>
		<xs:documentation>Загальні типи даних</xs:documentation>
	</xs:annotation>
	<!--Прості типи-->
	
  <!--тип "Код "виду 1.2.3.1"-->
