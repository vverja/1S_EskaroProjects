	<!--Загальний тип "Назва ДПІ, до якого подається копія податкового документа до відома"-->
	<xs:simpleType name="DGhSTI_copy">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
