<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:annotation>
		<xs:documentation>пнгпюусмнй яокюрх ╙дхмнцн ондюрйс яса'╙йрнл люкнцн о╡допх╙лмхжрбю - чпхдхвмнч нянанч</xs:documentation>
	</xs:annotation>
	<xs:simpleType name="DGMonth">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="12"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DGYear">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1991"/>
			<xs:maxInclusive value="2099"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DGPType">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="5"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="DGsti">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="99"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DGchk">
		<xs:restriction base="xs:int">
			<xs:enumeration value="1"/>
		</xs:restriction>
	</xs:simpleType>
	 <xs:simpleType name="DGLong">
		<xs:restriction base="xs:unsignedLong">
			<xs:minInclusive value="1000"/>
			<xs:maxInclusive value="9999999999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DGDate">
		<xs:restriction base="xs:string">
			<xs:length value="8"/>
			<xs:pattern value="(((((0[1-9]|[1-2][0-9])(0(1|[3-9])|1[0-2]))|(30(0(1|[3-9])|1[0-2]))|(31(01|03|05|07|08|10|12)))(19|20)\d{2})|((0[1-9]|[1-2][0-9])02(19|20)[0-9](0|4))|((0[1-9]|[1-2][0-8]|19)02(19|20)[0-9](1|2|3|5|6|7|8|9)))"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DGkved">
          <xs:restriction base="xs:string">
          <xs:length value="7"/>
         <xs:pattern value="([0-9])([0-9])(\.)([0-9])([0-9])(\.)([0-9])"/>
         </xs:restriction>
     </xs:simpleType>
	<xs:element name="DECLAR" type="DeclarContent"/>
	<xs:complexType name="DeclarContent">
		<xs:sequence>
			<xs:element name="DECLARHEAD" type="DHead" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DECLARBODY" type="DBody" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DHead">
		<xs:sequence>
			<xs:element name="TIN" type="DGLong" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC" type="xs:string" minOccurs="1" maxOccurs="1" fixed="J01"/>
			<xs:element name="C_DOC_SUB" type="xs:string" minOccurs="1" maxOccurs="1" fixed="035"/>
			<xs:element name="C_DOC_VER" type="xs:string" minOccurs="1" maxOccurs="1" fixed="2"/>
			<xs:element name="C_DOC_TYPE" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC_CNT" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_REG" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_RAJ" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_MONTH" type="DGMonth" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_TYPE" type="DGPType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_YEAR" type="DGYear" minOccurs="1" maxOccurs="1"/>
			<xs:element name="D_FILL" type="DGDate" minOccurs="1" maxOccurs="1"/>
			<xs:element name="SOFTWARE" type="xs:string" nillable="true" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DBody">
		<xs:sequence>
			<!--header-->
			<xs:choice>
			<xs:element name="HZ" type="DGchk" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HZN" type="DGchk" nillable="true" minOccurs="0" maxOccurs="1"/>
			</xs:choice>
			<xs:element name="HZY" type="DGYear" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:choice>
			<xs:element name="H1KV" type="DGchk" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HHY" type="DGchk" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="H3KV" type="DGchk" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HY" type="DGchk" nillable="true" minOccurs="0" maxOccurs="1"/>
			</xs:choice>
			<xs:element name="HNAME" type="xs:string" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HTIN" type="DGLong" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HKVED" type="DGkved" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HLOC" type="xs:string" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HZIP" type="xs:int" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HTEL" type="xs:string" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HEMAIL" type="xs:string" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HFAX" type="xs:string" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HSTI" type="xs:string" nillable="true" minOccurs="0" maxOccurs="1"/>
			<!--body-->
			<xs:element name="R010G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="R020G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="R030G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="R040G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="R050G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="R050G1" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="R060G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="R090G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="R070G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="R080G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="R100G3" type="xs:decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<!--footer-->
			<xs:element name="HFILL" type="DGDate" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HBOS" type="xs:string" nillable="true" minOccurs="0" maxOccurs="1"/>
			<xs:element name="HBUH" type="xs:string" nillable="true" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
