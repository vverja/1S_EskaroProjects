<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:annotation>
		<xs:documentation>Податковий розрахунок сум доходу, нарахованого (сплаченого) на користь платників податку, і сум утриманого з них податку [Форма № 1 ДФ](Затверджено наказом ДПА України від 29.09.2003 р. N 451)</xs:documentation>
	</xs:annotation>

 <xs:include schemaLocation="common_types.xsd" />

	<xs:element name="DECLAR" type="DeclarContent"/>
	<xs:complexType name="DeclarContent">
		<xs:sequence>
			<xs:element name="DECLARHEAD" type="DHead" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DECLARBODY" type="DBody" minOccurs="1" maxOccurs="1">
				<xs:unique name="URXXXXG02"><xs:selector xpath="RXXXXG02"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="URXXXXG03A"><xs:selector xpath="RXXXXG03A"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="URXXXXG03"><xs:selector xpath="RXXXXG03"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="URXXXXG04A"><xs:selector xpath="RXXXXG04A"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="URXXXXG04"><xs:selector xpath="RXXXXG04"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="URXXXXG05"><xs:selector xpath="RXXXXG05"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="URXXXXG06D"><xs:selector xpath="RXXXXG06D"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="URXXXXG07D"><xs:selector xpath="RXXXXG07D"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="URXXXXG08"><xs:selector xpath="RXXXXG08"/><xs:field xpath="@ROWNUM"/></xs:unique>
				<xs:unique name="URXXXXG09"><xs:selector xpath="RXXXXG09"/><xs:field xpath="@ROWNUM"/></xs:unique>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DHead">
		<xs:sequence>
			<xs:element name="TIN" type="DGLong" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC" type="xs:string" minOccurs="1" maxOccurs="1" fixed="F05"/>
			<xs:element name="C_DOC_SUB" type="xs:string" minOccurs="1" maxOccurs="1" fixed="001"/>
			<xs:element name="C_DOC_VER" type="xs:string" minOccurs="1" maxOccurs="1" fixed="2"/>
			<xs:element name="C_DOC_TYPE" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC_CNT" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_REG" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_RAJ" type="DGsti" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_MONTH" type="DGMonth" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_TYPE" type="DGPType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="PERIOD_YEAR" type="DGYear" minOccurs="1" maxOccurs="1"/>
			<xs:element name="C_DOC_STAN" type="DGSTAN" minOccurs="1" maxOccurs="1"/>
			<xs:element name="D_FILL" type="DGDate" minOccurs="1" maxOccurs="1"/>
			<xs:element name="SOFTWARE" type="xs:string"  nillable="true" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DBody">
		<xs:sequence>
			<!--HEAD-->
			<xs:element name="HTIN" type="DGHTINF"	minOccurs="1" maxOccurs="1"/>
			<xs:element name="HPAGES" type="xs:nonNegativeInteger"	nillable="true" minOccurs="0" maxOccurs="1"/>

			<xs:choice minOccurs="1" maxOccurs="1">
			<xs:element name="HZ" type="DGchk"  />
			<xs:element name="HZN" type="DGchk"  />
			<xs:element name="HZU" type="DGchk"  />
			</xs:choice>
			<xs:choice minOccurs="1" maxOccurs="1">
			<xs:element name="HJ" type="DGchk"  />
			<xs:element name="HF" type="DGchk"  />
			</xs:choice>

			<xs:element name="HNAME" type="DGHNAME"  minOccurs="1" maxOccurs="1"/>
			<xs:element name="HTINSTI" type="DGNLong"  nillable="true" minOccurs="0" />

			<xs:element name="HSTI" type="DGHSTI"  minOccurs="1" maxOccurs="1"/>
			<xs:element name="HZP" type="DGKv"  minOccurs="1" maxOccurs="1"/>
			<xs:element name="HZY" type="DGYear"  minOccurs="1" maxOccurs="1"/>

			<!--BODY-->
			<xs:element name="R00G01I" type="xs:nonNegativeInteger"  nillable="true" minOccurs="0" />
			<xs:element name="R00G02I" type="xs:nonNegativeInteger"  nillable="true" minOccurs="0" />

			<!--TABLE №1-->

			<xs:element name="RXXXXG02" type="TinColumn"  nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="RXXXXG03A" type="Decimal2Column"  nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="RXXXXG03" type="Decimal2Column"  nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="RXXXXG04A" type="Decimal2Column"  nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="RXXXXG04" type="Decimal2Column"  nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="RXXXXG05" type="OdohColumn"  nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="RXXXXG06D" type="DateColumn"	nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="RXXXXG07D" type="DateColumn"	nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="RXXXXG08" type="OspColumn"  nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="RXXXXG09" type="OznColumn"  nillable="true" minOccurs="0" maxOccurs="9999"/>

			<!--/TABLE №1-->

			<xs:element name="R01G03A" type="DGdecimal2"  nillable="true" minOccurs="0" />
			<xs:element name="R01G03" type="DGdecimal2"  nillable="true" minOccurs="0" />
			<xs:element name="R01G04A" type="DGdecimal2"  nillable="true" minOccurs="0" />
			<xs:element name="R01G04" type="DGdecimal2"  nillable="true" minOccurs="0" />

			<!--FOOTER-->
			<xs:element name="R02G01I" type="xs:nonNegativeInteger"  nillable="true" minOccurs="0" />
			<xs:element name="R02G02I" type="xs:nonNegativeInteger"  nillable="true" minOccurs="0" />
			<xs:element name="R02G03I" type="xs:nonNegativeInteger"  nillable="true" minOccurs="0" />

			<xs:element name="HKBOS" type="DGLong"	nillable="true" minOccurs="0" />
			<xs:element name="HBOS" type="DGHBOS"  nillable="true" minOccurs="0" />
			<xs:element name="HTELBOS" type="xs:string"  nillable="true" minOccurs="0" />

			<xs:element name="HKBUH" type="DGLong"	nillable="true" minOccurs="0" />
			<xs:element name="HBUH" type="DGHBUH"  nillable="true" minOccurs="0" />
			<xs:element name="HTELBUH" type="xs:string"  nillable="true" minOccurs="0" />

			<xs:element name="HFO" type="DGHFO" minOccurs="1" maxOccurs="1"  />
			<xs:element name="HTELFO" type="xs:string"  nillable="true" minOccurs="0" />

			<xs:element name="HFILL" type="DGDate"	minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
